{
	"name": "svelte-vsc",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build --cname temporarily-permanent.dev",
		"wireframe:png": "node render_wireframe_png.mjs"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"gh-pages": "^6.3.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sharp": "^0.34.4",
		"svelte": "^5.34.3",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@sveltestrap/sveltestrap": "^7.1.0",
		"svelte-i18n": "^4.0.0",
		"svelte-inview": "^4.0.2"
	}
}
